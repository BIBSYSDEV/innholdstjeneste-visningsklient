{
  "name": "innholdstjeneste-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "REACT_APP_USE_MOCK=false react-scripts build",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000",
    "test": "cypress run",
    "start:development": "env-cmd -f ./.env.development npm run-script start",
    "start:production": "env-cmd -f ./.env.production npm run-script start"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "env-cmd": "^10.1.0",
    "iframe-resizer": "^4.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@cypress/code-coverage": "^3.14.6",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "axios-mock-adapter": "^1.22.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^4.2.5",
    "cypress": "^15.3.0",
    "eslint-plugin-cypress": "^3.6.0",
    "chokidar": "^3.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.3.1",
    "typescript": "^4.9.5",
    "start-server-and-test": "^2.0.13"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
