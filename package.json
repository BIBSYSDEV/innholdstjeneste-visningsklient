{
  "name": "innholdstjeneste-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "REACT_APP_USE_MOCK=false react-scripts build",
    "test:cypress": "REACT_APP_USE_MOCK=true start-server-and-test http://localhost:3000",
    "test": "cypress run",
    "start:development": "env-cmd -f ./.env.development npm run-script start",
    "start:production": "env-cmd -f ./.env.production npm run-script start"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "env-cmd": "^10.1.0",
    "iframe-resizer": "^4.3.2",
    "react": "^18.1.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-scripts": "5.0.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@cypress/instrument-cra": "^1.4.0",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "axios-mock-adapter": "^1.21.2",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "cypress": "^10.6.0",
    "eslint-plugin-cypress": "^2.12.1",
    "chokidar": "^3.5.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.8.2",
    "start-server-and-test": "^1.14.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
